
<view class="page-body">
  <view class="maptext">
    腾讯地图
  </view>
  <view class="viewMap">
    <map id="map" style="width: 100%; height: 800rpx;" latitude="{{latitude}}" longitude="{{longitude}}" markers="{{markers}}" polyline="{{polyline}}" skew="30" enable-3D show-compass scale="13" show-location show-scale enable-traffic  subkey="SK2BZ-MLE3X-JCS4U-TAYLY-HLQ3J-LYBKJ"></map>
  </view>
  <view class="btn-area">
    <button bindtap="getCenterLocation" class="page-body-button" type="primary">获取地图中心位置坐标</button>
    <button bindtap="moveToLocation" class="page-body-button" type="primary">获取当前位置</button>
   
  </view>
</view>
